/*<meta />*/

body
{
	font-family: Arial, sans-serif;
}

h1.home
{
	text-align: center;
}

img
{
	border-radius: 1rem;
	max-width: 100%;
}

div.home
{
	mc-grid-row: true;
	margin-left: auto;
	margin-right: auto;
}

div.home::before
{
	content: ' ';
	display: table;
}

div.home::after
{
	content: ' ';
	display: table;
	clear: both;
}

div.home > div
{
	float: left;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	padding: 1rem;
}

div.home div p
{
	font-weight: bold;
	text-align: center;
}

div.home > div:nth-child(1)
{
	width: 33.33333%;
	margin-left: 0%;
}

div.home > div:nth-child(2)
{
	width: 33.33333%;
	margin-left: 0%;
}

div.home > div:nth-child(3)
{
	width: 33.33333%;
	margin-left: 0%;
}

MadCap|dropDown
{
	
}

MadCap|dropDownHead
{
	margin-top: 2em;
}

MadCap|dropDownHotspot
{
	font-weight: bold;
}

MadCap|breadcrumbsProxy
{
	border-bottom-width: 5px;
	mc-breadcrumbs-divider: '砻קยด1ตว融';
	mc-breadcrumbs-prefix: '砻קยด2ตว融';
}

@media tablet
{
	div.home > div:nth-child(1)
	{
		width: 33.33333%;
		margin-left: 0%;
	}

	div.home > div:nth-child(2)
	{
		width: 33.33333%;
		margin-left: 0%;
	}

	div.home > div:nth-child(3)
	{
		width: 33.33333%;
		margin-left: 0%;
	}
}

@media mobile
{
	div.home > div:nth-child(1)
	{
		width: 100%;
		margin-left: 0%;
	}

	div.home > div:nth-child(2)
	{
		width: 100%;
		margin-left: 0%;
	}

	div.home > div:nth-child(3)
	{
		width: 100%;
		margin-left: 0%;
	}
}

